generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model User {
    userId        String   @id @default(cuid())
    email         String   @unique
    first_name    String
    last_name     String?
    gender        String
    date_of_birth DateTime
}
